{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "ext-amqp": "*",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "broadway/broadway-bundle": "^0.5.0",
    "broadway/event-store-dbal": "^0.5.0",
    "broadway/read-model-elasticsearch": "^0.7.1",
    "composer/package-versions-deprecated": "1.11.99.1",
    "doctrine/doctrine-bundle": "^2.2",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "doctrine/orm": "^2.8",
    "elasticsearch/elasticsearch": "^7.10",
    "k911/swoole-bundle": "^0.9.0",
    "lexik/jwt-authentication-bundle": "^2.11",
    "liip/monitor-bundle": "^2.14",
    "moneyphp/money": "^3.3",
    "nelmio/api-doc-bundle": "^4.1",
    "php-amqplib/php-amqplib": "^2.12",
    "pixelfederation/doctrine-resettable-em-bundle": "^1.3.9",
    "ramsey/uuid": "^4.1",
    "ramsey/uuid-doctrine": "^1.6",
    "sensio/framework-extra-bundle": "^5.6",
    "sensiolabs/security-checker": "^6.0",
    "symfony/asset": "5.2.*",
    "symfony/console": "5.2.*",
    "symfony/dotenv": "5.2.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.2.*",
    "symfony/messenger": "5.2.*",
    "symfony/monolog-bundle": "^3.6",
    "symfony/proxy-manager-bridge": "5.2.*",
    "symfony/security-bundle": "5.2.*",
    "symfony/twig-bundle": "5.2.*",
    "symfony/validator": "5.2.*",
    "symfony/yaml": "5.2.*",
    "yoanm/jsonrpc-server-doc-sdk": "^0.2.1",
    "yoanm/symfony-jsonrpc-http-server": "^v2.0"
  },
  "require-dev": {
    "ext-xdebug": "*",
    "behat/behat": "^3.8",
    "blackfire/php-sdk": "^1.24",
    "coduo/php-matcher": "^6.0",
    "dvdoug/behat-code-coverage": "^5.2",
    "friends-of-behat/symfony-extension": "^2.0",
    "friends-of-phpspec/phpspec-code-coverage": "^6.0",
    "friendsofphp/php-cs-fixer": "^2.18",
    "php-parallel-lint/php-parallel-lint": "^1.2",
    "phpro/grumphp": "^1.3",
    "phpspec/phpspec": "^7.0",
    "phpstan/phpstan": "^0.12.70",
    "qossmic/deptrac-shim": "^0.11.1",
    "seld/jsonlint": "^1.8",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/browser-kit": "5.2.*",
    "symfony/debug-bundle": "5.2.*",
    "symfony/maker-bundle": "^1.28",
    "symfony/stopwatch": "^5.2",
    "symfony/web-profiler-bundle": "^5.2"
  },
  "config": {
    "bin-dir": "bin",
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "files": [
      "src/Infrastructure/Symfony5/Kernel.php"
    ],
    "psr-4": {
      "Romaind\\PizzaStore\\": "src/"
    },
    "classmap": [
      "src/Infrastructure/Symfony5/Kernel.php"
    ],
    "exclude-from-classmap": [
      "**/tests/",
      "**/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Unit\\Romaind\\PizzaStore\\": "tests/Unit",
      "Tests\\Functional\\": "tests/Functional"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "build-test": [
      "@composer install --prefer-dist --optimize-autoloader --ansi --verbose"
    ],
    "behat-run": [
      "php -d memory_limit=1G -d date.timezone=\"Europe/Paris\" bin/behat --colors -vvv"
    ],
    "build": [
      "@composer install --prefer-dist --optimize-autoloader --ansi --verbose --no-scripts"
    ],
    "cache-clear-test": [
      "console --ansi --no-interaction cache:clear --env=test"
    ],
    "db-create": [
      "console --ansi doctrine:database:create --if-not-exists"
    ],
    "db-create-test": [
      "console --ansi doctrine:database:create --if-not-exists --env=test"
    ],
    "db-drop": [
      "console --ansi doctrine:database:drop --force"
    ],
    "db-drop-test": [
      "console --ansi doctrine:database:drop --force --env=test"
    ],
    "db-migrate": [
      "console --ansi doctrine:migrations:migrate --no-interaction --allow-no-migration"
    ],
    "db-migrate-test": [
      "console --ansi doctrine:migrations:migrate --no-interaction --env=test"
    ],
    "db-schema-validate": [
      "console --ansi doctrine:schema:validate"
    ],
    "deptrac": [
      "deptrac analyze --formatter-graphviz=0"
    ],
    "healthz": [
      "console --ansi monitor:health"
    ],
    "lint": [
      "@php-cs-fixer"
    ],
    "test": [
      "@cache-clear-test",
      "@test-unit",
      "@test-functional",
      "@test-static",
      "@test-layer"
    ],
    "test-functional": [
      "@db-create-test",
      "@db-migrate-test",
      "@behat-run"
    ],
    "test-layer": [
      "@deptrac"
    ],
    "test-unit": [
      "phpspec --no-interaction run -fpretty"
    ],
    "test-static": [
      "@phpstan"
    ],
    "php-cs-fixer": [
      "php-cs-fixer fix --config=.php_cs.php --verbose --diff"
    ],
    "phpstan": [
      "php -d memory_limit=1G bin/phpstan analyse -l 6 -c phpstan.neon src"
    ],
    "rmq-setup": [
      "console messenger:setup-transports --no-interaction"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.2.*"
    }
  }
}
